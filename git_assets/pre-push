#!/usr/bin/env python
import os
from subprocess import call

cwd = os.getcwd()

if call(["python", cwd + "/git_assets/pre-push-runner.py"]) == 0:
  print 'Tests passed successfully.'
else:
  print 'Some tests failed :('





